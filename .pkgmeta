package-as: DBM-Core

externals:
  DBM-Core/DBM-Core: https://github.com/DeadlyBossMods/DBM-Unified/tree/master/DBM-Core
  DBM-Core/Libs/CallbackHandler-1.0: https://repos.wowace.com/wow/callbackhandler/trunk/CallbackHandler-1.0
  DBM-Core/Libs/LibChatAnims: https://repos.wowace.com/wow/libchatanims/trunk/LibChatAnims
  DBM-Core/Libs/LibDBIcon-1.0: https://repos.wowace.com/wow/libdbicon-1-0/trunk/LibDBIcon-1.0
  DBM-Core/Libs/LibDurability: https://repos.wowace.com/wow/libdurability/trunk/LibDurability
  DBM-Core/Libs/LibLatency: https://repos.wowace.com/wow/liblatency/trunk/LibLatency
  DBM-Core/Libs/LibSharedMedia-3.0: https://repos.wowace.com/wow/libsharedmedia-3-0/trunk/LibSharedMedia-3.0
  DBM-Core/Libs/LibStub: https://repos.wowace.com/wow/libstub/trunk
  DBM-Core/Libs/LibDataBroker-1.1: https://github.com/tekkub/libdatabroker-1-1
  DBM-Core/Libs/LibDeflate: https://github.com/SafeteeWoW/LibDeflate
  DBM-Core/Libs/LibSerialize: https://github.com/rossnichols/LibSerialize

ignore:
  - DBM-Core/!SortList.txt
  - DBM-Core/Boss Mods.luadoc
  - DBM-Core/Libs/LibDeflate/docs
  - DBM-Core/Libs/LibDeflate/examples
  - DBM-Core/Libs/LibDeflate/LibDeflate.toc
  - DBM-Core/Libs/LibDeflate/README.md
  - DBM-Core/Libs/LibDeflate/CONTRIBUTING.md
  - DBM-Core/Libs/LibDeflate/changelog.md
  - DBM-Core/Libs/LibStub/tests
  - DBM-Core/Libs/LibDataBroker-1.1/README.textile

enable-nolib-creation: no

move-folders:
  DBM-Core/DBM-Brawlers: DBM-Brawlers
  DBM-Core/DBM-DefaultSkin: DBM-DefaultSkin
  DBM-Core/DBM-DMF: DBM-DMF
  DBM-Core/DBM-GUI: DBM-GUI
  DBM-Core/DBM-StatusBarTimers: DBM-StatusBarTimers
  DBM-Core/DBM-WorldEvents: DBM-WorldEvents
  DBM-Core/DBM-Party-Shadowlands: DBM-Party-Shadowlands
  DBM-Core/DBM-Shadowlands: DBM-Shadowlands
  DBM-Core/DBM-CastleNathria: DBM-CastleNathria
  DBM-Core/DBM-SanctumOfDomination: DBM-SanctumOfDomination
  DBM-Core/DBM-Core: DBM-Core
